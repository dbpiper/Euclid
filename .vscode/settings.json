{
  "editor.fontFamily": "'Source Code Pro', Consolas, 'Courier New', 'monospace'",
  "editor.tabSize": 2,
  "files.eol": "\n",
  "cSpell.words": [
    "aapl",
    "app",
    "browserslist",
    "commitlint",
    "dropdown",
    "eslint",
    "fixes",
    "flexbugs",
    "flowtype",
    "gitattributes",
    "graphql",
    "indicator",
    "jsdom",
    "linebreak",
    "markdownlint",
    "noopener",
    "noreferrer",
    "plugin",
    "polyfill",
    "postcss",
    "react",
    "refetch",
    "unencrypted"
  ],
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "eslint.workingDirectories": [
    "./src/client", "./src/server"
  ]
}
