{
  "author": "",
  "bugs": {
    "url": "https://github.com/dbpiper/Euclid/issues"
  },
  "dependencies": {
    "acorn": "^6.1.0",
    "acorn-jsx": "^5.0.1",
    "jest-cli": "^24.1.0",
    "start-server-and-test": "^1.7.11"
  },
  "description": "A website dedicated to tracking PC part price information.",
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.0",
    "babel-eslint": "^10.0.1",
    "commitlint": "^7.5.0",
    "cypress": "^3.1.5",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^7.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-euclid": "0.0.8",
    "typescript": "^3.3.3"
  },
  "homepage": "https://github.com/dbpiper/Euclid#readme",
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run preCommit",
      "pre-push": "npm run preCommit"
    }
  },
  "license": "MIT",
  "main": "index.js",
  "name": "euclid",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbpiper/Euclid.git"
  },
  "scripts": {
    "clientTestCypress": "cd src/client && npx cypress run",
    "euclid-test-e2e": "server-test startServer http://localhost:4000 startClientTest",
    "installClient": "cd src/client && npm install",
    "installClientFromLock": "cd src/client && npm ci",
    "installFromLock": "npm ci && npm run postinstallFromLock",
    "installServer": "cd src/server && npm install",
    "installServerFromLock": "cd src/server && npm ci",
    "installStandard": "npm install && npm run postinstallStandard",
    "lint-and-unit-test": "npx git status && npm run preCommitClient && npm run preCommitServer",
    "postinstallFromLock": "npm run installClientFromLock && npm run installServerFromLock",
    "postinstallStandard": "npm run installClient && npm run installServer",
    "preCommit": "npm run lint-and-unit-test && npm run euclid-test-e2e",
    "preCommitClient": "cd src/client && npm run preCommit",
    "preCommitServer": "cd src/server && npm run preCommit",
    "startClient": "cd src/client && npm run start",
    "startClientTest": "server-test startClient http://localhost:5000 clientTestCypress",
    "startServer": "cd src/server && npm run start -- production"
  },
  "version": "0.0.0"
}
