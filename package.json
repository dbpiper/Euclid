{
  "author": "",
  "bugs": {
    "url": "https://github.com/dbpiper/Euclid/issues"
  },
  "dependencies": {
    "acorn": "^6.1.1",
    "acorn-jsx": "^5.0.1",
    "gulp": "^4.0.0",
    "jest-cli": "^24.5.0",
    "start-server-and-test": "^1.7.12"
  },
  "description": "A website dedicated to tracking PC part price information.",
  "devDependencies": {
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.11",
    "babel-eslint": "^10.0.1",
    "commitlint": "^7.5.2",
    "cypress": "^3.2.0",
    "eslint": "^5.15.2",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^7.1.0",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "prettier": "^1.16.4",
    "tslint": "^5.14.0",
    "tslint-config-euclid": "1.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3333"
  },
  "homepage": "https://github.com/dbpiper/Euclid#readme",
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run preCommit",
      "pre-push": "npm run preCommit"
    }
  },
  "license": "MIT",
  "main": "index.js",
  "name": "euclid",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbpiper/Euclid.git"
  },
  "scripts": {
    "cypress:e2e:launch-run": "server-test startServer http://localhost:4000 startClientTest",
    "cypress:e2e:run": "cd src/client && npm run cypress:e2e:run",
    "cypress:run-all": "npm run cypress:storybook:launch-run && npm run cypress:e2e:launch-run",
    "cypress:storybook:launch-run": "server-test startStorybook http://localhost:6006 cypress:storybook:run",
    "cypress:storybook:run": "cd src/client && npm run cypress:storybook:run",
    "gitStatus": "npx git status && sleep 8",
    "installClient": "cd src/client && npm install",
    "installClientFromLock": "cd src/client && npm ci",
    "installFromLock": "npm ci && npm run postinstallFromLock",
    "installServer": "cd src/server && npm install",
    "installServerFromLock": "cd src/server && npm ci",
    "installStandard": "npm install && npm run postinstallStandard",
    "lint-and-unit-test": "npm run preCommitClient && npm run preCommitServer",
    "postinstallFromLock": "npm run installClientFromLock && npm run installServerFromLock",
    "postinstallStandard": "npm run installClient && npm run installServer",
    "preCommit": "npm run gitStatus && npm run lint-and-unit-test && npm run cypress:run-all",
    "preCommitClient": "cd src/client && npm run preCommit",
    "preCommitServer": "cd src/server && npm run preCommit",
    "startClient": "cd src/client && npm run start",
    "startClientTest": "server-test startClient http://localhost:5000 cypress:e2e:run",
    "startServer": "cd src/server && npm run startProduction",
    "startStorybook": "cd src/client && npm run storybook"
  },
  "version": "0.0.0"
}
