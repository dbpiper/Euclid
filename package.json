{
  "author": "",
  "bugs": {
    "url": "https://github.com/dbpiper/Euclid/issues"
  },
  "dependencies": {
    "jest-cli": "^24.1.0"
  },
  "description": "A website dedicated to tracking PC part price information.",
  "devDependencies": {
    "@commitlint/cli": "^7.5.0",
    "@commitlint/config-conventional": "^7.5.0",
    "@types/jest": "^24.0.0",
    "babel-eslint": "^10.0.1",
    "commitlint": "^7.5.0",
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-unicorn": "^7.1.0",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-euclid": "0.0.8",
    "typescript": "^3.3.3"
  },
  "homepage": "https://github.com/dbpiper/Euclid#readme",
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run preCommit",
      "pre-push": "npm run preCommit"
    }
  },
  "license": "MIT",
  "main": "index.js",
  "name": "euclid",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dbpiper/Euclid.git"
  },
  "scripts": {
    "installClient": "cd src/client && npm install",
    "installServer": "cd src/server && npm install",
    "installSubmodules": "npm run installClient && npm run installServer",
    "preCommit": "npx git status && npm run preCommitClient && npm run preCommitServer",
    "preCommitClient": "cd src/client && npm run preCommit",
    "preCommitServer": "cd src/server && npm run preCommit"
  },
  "version": "0.0.0"
}
