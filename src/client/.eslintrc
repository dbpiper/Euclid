{
  "extends": [
    "airbnb",
    "plugin:jest/recommended",
    "plugin:unicorn/recommended"
  ],
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": [
      1, {
        "extensions": [
          ".js", ".jsx"
        ]
      }
    ],
    "unicorn/filename-case": [
      0
    ]
  },
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
    "module-resolver",
    "jest"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {
        "root": ["./src"],
        "alias": {
          "App": ["./src/screens/App"],
          "config": ["./src/config"]
        }
      }
    }
  },
  "parser": "babel-eslint"
}
