{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "airbnb",
    "plugin:jest/recommended",
    "plugin:unicorn/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018
  },
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
    "module-resolver",
    "jest"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "unicorn/filename-case": [
      0
    ]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "alias": {
          "App": [
            "./src/screens/App"
          ],
          "config": [
            "./src/config"
          ]
        },
        "root": [
          "./src"
        ]
      }
    }
  }
}
