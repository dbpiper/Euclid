{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "extends": [
    "plugin:jest/recommended",
    "plugin:unicorn/recommended",
    "airbnb"
  ],
  "plugins": [
    "react",
    "import",
    "jsx-a11y",
    "module-resolver",
    "jest",
    "cypress"
  ],
  "rules": {
    "no-magic-numbers": "error",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    ],
    "unicorn/filename-case": [
      0
    ]
  },
  "settings": {
    "import/resolver": {
      "babel-module": {
        "alias": {
          "App": [
            "./src/screens/App"
          ],
          "config": [
            "./src/config"
          ]
        },
        "root": [
          "./src"
        ]
      },
      "node": {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    }
  }
}
