    "@types/lodash": "^4.14.120",
    "@types/ramda": "^0.25.48",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/react-select": "^2.0.11",
    "@types/recharts-scale": "^1.0.0",
    "@types/redux": "^3.6.0",
    "@types/styled-components": "^4.1.8",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.1.0",
    "d3-shape": "^1.3.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-unicorn": "^7.1.0",
    "jest": "^24.1.0",
    "jest-environment-enzyme": "^7.0.1",
    "jest-enzyme": "^7.0.1",
    "jest-styled-components": "^6.3.1",
    "recharts-scale": "^0.4.2",
    "redux-test-utils": "^0.3.0",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy",
      "^react-native$": "react-native-web"
    },
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "jest-enzyme"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "enzyme",
    "testEnvironmentOptions": {
      "enzymeAdapter": "react16"
    },
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testURL": "http://localhost",
    "transform": {
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js",
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  },
  "name": "client",
  "private": true,
  "scripts": {
    "build": "node scripts/build.js",
    "check-types": "tsc",
    "lint": "npm run lintES && npm run lintTS",
    "lintES": "npx eslint .",
    "lintTS": "npx tslint \"./**/*.ts?(x)\" \"src/**/*.ts?(x)\" --project tsconfig.json",
    "preCommit": "npm run build && npm run staticCheckAndTestVerify",
    "start": "node scripts/start.js",
    "staticCheck": "npm run lint && npm run check-types",
    "staticCheckAndTestVerify": "npm run staticCheck && npm run testVerify",
    "test": "node scripts/test.js --watch",
    "testVerify": "node scripts/test.js",
    "testWatch": "node scripts/test.js --watch"
  },
  "version": "0.1.0"
}
