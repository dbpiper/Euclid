{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "*": [
        "*",
        "config/@types/*"
      ],
      "App": [
        "screens/App"
      ],
      "App/*": [
        "screens/App/*"
      ],
      "shared": [
        "screens/App/shared",
        "screens/App/screens/Home/shared"
      ],
      "shared/*": [
        "screens/App/shared/*",
        "screens/App/screens/Home/shared/*"
      ],
    }
  },
  "extends": "../../tsconfig-base",
  "include": [
    "src",
    "./src/**/*"
  ],
  "types": [
    "jest",
    "enzyme"
  ]
}
