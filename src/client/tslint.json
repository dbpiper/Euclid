{
  "defaultSeverity": "error",
  "extends": [
    "tslint-config-euclid"
  ],
  "jsRules": {},
  "linterOptions": {
    "exclude": [
      "**/node_modules/**",
      "src/config/@types/**"
    ]
  },
  "rules": {
    "jsx-no-multiline-js": false,
    "no-angle-bracket-type-assertion": true,
    "no-any": {
      "severity": "warning"
    },
    "prefer-conditional-expression": false,
    "no-implicit-dependencies": [
      true,
      "dev",
      [
        "App",
        "config",
        "shared",
        "src"
      ]
    ],
    "no-submodule-imports": [
      true,
      "App",
      "config",
      "shared",
      "src"
    ],
    "no-unsafe-any": {
      "severity": "error"
    }
  }
}
