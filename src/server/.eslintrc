{
  "extends": "airbnb",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      1, {
        "extensions": [
          ".js", ".jsx"
        ]
      }
    ]
  },
  "plugins": [
    "react", "import", "jsx-a11y", "module-resolver"
  ],
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
