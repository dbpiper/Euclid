type Stock {
  id: ID! @unique
  price: Float!
  date: Int!
}

type Stocks {
  id: ID! @unique
  stocks: [Stock!]!
  ticker: String!
}

type User {
  id: ID! @unique
  email: String @unique
  name: String!
  posts: [Post!]!
}

type Post {
  id: ID! @unique
  title: String!
  published: Boolean! @default(value: "false")
  author: User
}
