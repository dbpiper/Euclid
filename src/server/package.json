    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nodemon": "^1.18.10",
    "prisma": "^1.26.4",
    "prisma-client-lib": "^1.26.4",
    "ramda": "^0.26.1",
    "util": "^0.11.1"
