{
  "author": "David Piper",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.3.1",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "async": "^2.6.1",
    "es6-promise": "^4.2.5",
    "graphql": "^14.1.1",
    "graphql-yoga": "^1.17.2",
    "iex-api": "0.0.3",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "nodemon": "^1.18.9",
    "prisma": "^1.25.7",
    "prisma-client-lib": "^1.25.7",
    "ramda": "^0.26.1"
  },
  "description": "The server for Euclid",
  "devDependencies": {
    "@types/async": "^2.4.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/lodash": "^4.14.120",
    "@types/ramda": "^0.25.47",
    "@types/ws": "^6.0.1",
    "babel-register": "^6.26.0",
    "eslint": "^5.13.0",
    "eslint-import-resolver-node": "^0.3.2",
    "eslint-plugin-jest": "^22.2.2",
    "eslint-plugin-module-resolver": "^0.8.0",
    "eslint-plugin-unicorn": "^7.1.0",
    "jsdoc": "^3.5.5",
    "tslint": "^5.12.1",
    "typescript": "^3.3.1"
  },
  "license": "MIT",
  "main": "index.ts",
  "name": "server",
  "scripts": {
    "build": "npx @babel/cli --extensions \".ts\" src -d dist --source-maps",
    "check-types": "tsc",
    "lint": "npm run lintTS",
    "lintTS": "npx tslint \"./*.ts?(x)\" \"./src/**/*.ts?(x)\" \"./config/**/*.ts?(x)\" --project tsconfig.json",
    "preCommit": "npm run staticCheckAndTestVerify",
    "start": "babel-node index.ts --extensions \".ts\" ",
    "startNodemon": "nodemon --exec babel-node --extensions \".ts\" index.js",
    "staticCheck": "npm run lint && npm run check-types",
    "staticCheckAndTestVerify": "npm run staticCheck && npm run testVerify",
    "test": "echo \"Error: no test specified\" && exit 1",
    "testVerify": "exit 0"
  },
  "version": "1.0.0"
}
